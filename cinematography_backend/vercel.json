{
  "version": 2,
  "builds": [
    {
      "src": "cinematography_backend/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "50mb", 
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "cinematography_backend/wsgi.py"
    }
  ]
}